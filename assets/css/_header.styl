header
  padding: 44px 0

  .logo
    max-width: 60px
    float: left

  h1
    float: left
    margin: 0
    line-height: 35px
    padding-left: 15px
    color: #58585b
    letter-spacing: 0.05em
    text-transform: uppercase
    font-size: 2em

  nav
    float: right
    text-transform: uppercase

  .wrap
    group()

  nav
    ul
      inline-list()
    a
      color: #71c9bd
      font-weight: 500
      font-size: 1.4em
      hover-darken()
      transition: color .5s
      line-height: 35px
